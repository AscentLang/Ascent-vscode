{
  "name": "ascent",
  "displayName": "ascent",
  "description": "Extension to provide language features for the Ascent language",
  "version": "0.0.1",
  "icon": "assets/extension.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ascent",
      "aliases": ["Ascent", "ascent"],
      "extensions": [".asc"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./assets/icon.png",
        "dark": "./assets/icon-dark.png"
      }
    }],
    "grammars": [{
      "language": "ascent",
      "scopeName": "source.ascent",
      "path": "./syntaxes/ascent.tmLanguage.json"
    }]
  }
}
